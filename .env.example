# Database Configuration
db_host=your_database_host
db_port=3306
db_user=your_database_user
db_password=your_database_password
db_name=rwa
db_connection_limit=10
db_wait_for_connections=true
db_idleTimeout=30000

# Email Configuration (Office 365)
EMAIL_HOST=smtp.office365.com
EMAIL_PORT=587
EMAIL_USER=your_email@domain.com
EMAIL_PASSWORD=your_email_password

# JWT Secret (if using authentication)
jwt_SecretKey=your_jwt_secret_key

# SumSub API (KYC)
SUMSUB_APP_TOKEN=your_sumsub_app_token
SUMSUB_SECRET_KEY=your_sumsub_secret_key
SUMSUB_API_URL=https://api.sumsub.com

# Blockchain (Sepolia Testnet)
SEPOLIA_PRIVATE_KEY=your_private_key_1
SEPOLIA_PRIVATE_KEY2=your_private_key_2
SEPOLIA_PRIVATE_KEY3=your_private_key_3
