{
  "buildCommand": "cd Frontend/Website && npm run build",
  "outputDirectory": "Frontend/Website/dist",
  "installCommand": "npm install --prefix api && npm install --prefix Frontend/Website",
  "routes": [
    {
      "src": "/api/project/(.*)",
      "dest": "/api/project.js"
    },
    {
      "src": "/api/subscription/(.*)",
      "dest": "/api/subscription.js"
    },
    {
      "src": "/api/user/(.*)",
      "dest": "/api/user.js"
    },
    {
      "src": "/api/contact",
      "dest": "/api/contact.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
