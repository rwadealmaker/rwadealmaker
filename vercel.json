{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/Website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/transaction/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/project/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/Website/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
