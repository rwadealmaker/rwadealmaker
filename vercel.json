{
  "buildCommand": "cd Frontend/Website && npm run build",
  "outputDirectory": "Frontend/Website/dist",
  "installCommand": "npm install --prefix api && npm install --prefix Frontend/Website",
  "rewrites": [
    {
      "source": "/api/project/:path*",
      "destination": "/api/project"
    },
    {
      "source": "/api/subscription/:path*",
      "destination": "/api/subscription"
    },
    {
      "source": "/api/user/:path*",
      "destination": "/api/user"
    },
    {
      "source": "/api/contact",
      "destination": "/api/contact"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
