{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/Website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/Website/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/transaction/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/project/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/Website/dist/index.html"
    }
  ]
}
