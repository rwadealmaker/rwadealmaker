# Vercel 环境变量配置模板
# 在 Vercel 项目设置中添加这些环境变量

# 数据库配置 (MySQL)
db_host=your-mysql-host.com
db_port=3306
db_user=your_database_user
db_password=your_database_password
db_name=rwa

# Email 配置 (Office 365)
EMAIL_HOST=smtp.office365.com
EMAIL_PORT=587
EMAIL_USER=your_email@domain.com
EMAIL_PASSWORD=your_email_password

# JWT Secret
jwt_SecretKey=your_super_secret_jwt_key_here

# SumSub API (KYC) - 可选
SUMSUB_APP_TOKEN=your_sumsub_app_token
SUMSUB_SECRET_KEY=your_sumsub_secret_key
SUMSUB_API_URL=https://api.sumsub.com

# Blockchain (Sepolia Testnet) - 可选
SEPOLIA_PRIVATE_KEY=your_private_key_1
SEPOLIA_PRIVATE_KEY2=your_private_key_2
SEPOLIA_PRIVATE_KEY3=your_private_key_3

# Node Environment
NODE_ENV=production
